{
  "name": "prettier-plugin-pg",
  "version": "0.0.0",
  "description": "A prettier plugin for formatting PostgreSQL-flavour SQL",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "prettier": "prettier --plugin=. --parser=postgresql",
    "add-statement": "./scripts/add_statement.js"
  },
  "repository": {
    "type": "git",
    "url": "https+git://github.com/benjie/prettier-plugin-pg.git"
  },
  "keywords": [
    "prettier",
    "sql",
    "pgsql",
    "postgres",
    "postgresql",
    "plpgsql",
    "plv8",
    "psql"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.15.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.6.2",
    "eslint-plugin-prettier": "2.4.0",
    "jest": "22.0.6",
    "jest-runner-eslint": "0.3.0",
    "prettier": "1.10.2"
  },
  "dependencies": {
    "pg-query-native": "1.1.0"
  },
  "files": [
    "index.js",
    "embed.js",
    "print.js",
    "reservedWords.js"
  ],
  "jest": {
    "projects": [
      "<rootDir>/jest.*.config.js"
    ]
  }
}
